{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "lib": ["ESNext"],

    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitReturns": true,

    "esModuleInterop": true,
    "importsNotUsedAsValues": "preserve",

    "skipLibCheck": true,
    "skipDefaultLibCheck": true,

    "baseUrl": ".",
    "outDir": "./dist"
  },
}
